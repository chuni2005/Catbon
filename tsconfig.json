{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "target": "ES2020",
    "module": "ESNext",
    "resolveJsonModule": true,
    "types": ["vite/client"]
  },
  "include": ["src"],
  "vite": {
    "assetsInclude": ["**/*.svg", "**/*.png", "**/*.jpg", "**/*.jpeg", "**/*.gif"]
  }
}
